<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.TP3" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Paladins</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
        <icon density="ldpi" src="www/img/favicon/android-icon-36x36.png" />
        <icon density="mdpi" src="www/img/favicon/android-icon-48x48.png" />
        <icon density="hdpi" src="www/img/favicon/android-icon-72x72.png" />
        <icon density="xhdpi" src="www/img/favicon/android-icon-96x96.png" />
        <icon density="xxhdpi" src="www/img/favicon/android-icon-144x144.png" />
        <icon density="xxxhdpi" src="www/img/favicon/android-icon-192x192.png" />
        <splash density="land-hdpi" src="www/img/favicon/android-icon-96x96.png" />
        <splash density="port-hdpi" src="www/img/favicon/android-icon-192x192.png" />
    </platform>
    <preference name="SplashScreenDelay" value="10000" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" />
    <plugin name="cordova-plugin-network-information" spec="~2.0.1" />
    <plugin name="cordova-plugin-media" spec="~5.0.2" />
    <plugin name="cordova-plugin-battery-status" spec="~2.0.1" />
    <plugin name="cordova-plugin-globalization" spec="~1.0.9" />
    <plugin name="cordova-plugin-device" spec="~2.0.1" />
    <plugin name="cordova-plugin-contacts" spec="~3.0.1" />
    <engine name="android" spec="^7.0.0" />
</widget>
