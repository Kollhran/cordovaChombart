<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.TP3" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <feature name="Battery">
        <param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
    </feature>
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <name>Paladins</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="www/img/favicon/android-icon-36x36.png" />
    <icon density="mdpi" src="www/img/favicon/android-icon-48x48.png" />
    <icon density="hdpi" src="www/img/favicon/android-icon-72x72.png" />
    <icon density="xhdpi" src="www/img/favicon/android-icon-96x96.png" />
    <icon density="xxhdpi" src="www/img/favicon/android-icon-144x144.png" />
    <icon density="xxxhdpi" src="www/img/favicon/android-icon-192x192.png" />
    <splash density="land-hdpi" src="www/img/favicon/android-icon-96x96.png" />
    <splash density="port-hdpi" src="www/img/favicon/android-icon-192x192.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreenDelay" value="10000" />
</widget>
